{
  "name": "@fui/shared",
  "version": "0.1.0",
  "license": "MIT",
  "exports": "./mod.ts",
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "esnext"
