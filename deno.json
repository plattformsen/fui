{
  "imports": {
    "preact": "npm:preact@^10.27.2"
  },
  "nodeModulesDir": "auto",
  "workspace": {
    "members": [
      "components/*",
      "packages/*"
    ]
  },
  "publish": {
    "include": [
      "**/*.tsx"
    ],
    "exclude": [
      "**/*_test.tsx",
      "**/*_test.ts",
      "**/*.story.tsx",
      "**/*.stories.tsx",
      "**/*.mdx",
      "**/*.md"
    ]
  }
}
